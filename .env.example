NODE_ENV=development
NEXT_PUBLIC_SHOW_LOGGER=true

# 服务器监听地址
# 开发环境: localhost（仅本机访问）
# 生产环境: 0.0.0.0（允许外部访问）或具体的 IP
HOSTNAME=localhost

# 服务器端口
# 开发环境: 3000
# 生产环境: 80 或其他
PORT=3000

# 后端服务 URL（仅用于服务端代理，不会暴露给客户端）
# 开发环境: http://localhost:3001
# 生产环境: 后端服务的内网地址，如 http://backend:3001 或 http://127.0.0.1:3001
BACKEND_URL=http://localhost:3001

# JWT Token 存储的 key
JWT_TOKEN_KEY='auth-token'